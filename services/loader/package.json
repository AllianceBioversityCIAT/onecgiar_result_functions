{
  "name": "@prms/loader-s3",
  "version": "1.0.1",
  "type": "module",
  "main": "dist/handler.js",
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "build": "rm -rf dist && npx esbuild src/handler.ts --bundle --platform=node --target=node20 --format=cjs --outfile=dist/handler.js",
    "zip": "npm run build && (cd dist && zip -qr ../loader.zip .) && echo 'ZIP listo: loader.zip'"
  },
  "devDependencies": {
    "esbuild": "^0.23.1",
    "typescript": "^5.5.4",
    "@types/node": "^20.12.12",
    "@types/node-fetch": "^2.6.4"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.614.0",
    "@opensearch-project/opensearch": "^2.3.1",
    "node-fetch": "^3.3.2"
  }
}
