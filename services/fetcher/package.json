{
  "name": "@prms/normalizer",
  "version": "2.0.0",
  "private": true,
  "type": "module",
  "main": "dist/lambda.cjs",
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "build": "rm -rf dist && npx esbuild src/lambda.mjs --bundle --platform=node --target=node20 --format=cjs --outfile=dist/lambda.cjs --keep-names --log-level=info",
    "zip": "npm run build && (cd dist && zip -qr ../normalizer.zip .) && echo 'ZIP listo: normalizer.zip'"
  },
  "dependencies": {
    "@aws-sdk/client-eventbridge": "^3.899.0",
    "@aws-sdk/client-s3": "^3.899.0",
    "@vendia/serverless-express": "^4.12.6",
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "express": "^4.19.2",
    "node-fetch": "^3.3.2",
    "swagger-ui-express": "^5.0.1"
  },
  "devDependencies": {
    "esbuild": "^0.23.1"
  }
}
