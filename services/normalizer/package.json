{
  "name": "@prms/normalizer",
  "version": "1.0.1",
  "type": "module",
  "private": true,
  "main": "src/handlers/injest.mjs",
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "postinstall": "true",
    "zip": "rm -rf dist && mkdir -p dist && cp -R src package.json dist/ && cd dist && npm ci --omit=dev && zip -r ../normalizer.zip . && cd ..",
